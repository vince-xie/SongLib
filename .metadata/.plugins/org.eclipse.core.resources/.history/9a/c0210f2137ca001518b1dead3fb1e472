package model;

public class Song {
	
	private String name;
	private String album;
	private String artist;
	private int year;
	
	public Song(String name){
		this.name = name;
	}
	
	public Song(String name, String album){
		this(name);
		this.album = album;
	}
	
	public String toString(){
		return "Name: " + name + "\n Album: " + album + "Artist: " + artist + "\n Year: " + year;
	}
}
